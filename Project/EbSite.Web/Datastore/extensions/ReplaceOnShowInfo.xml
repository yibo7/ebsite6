<?xml version="1.0" encoding="utf-8"?>
<ManagedExtension xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="ReplaceOnShowInfo">
  <BaseType />
  <Version>1.5</Version>
  <Priority>999</Priority>
  <Description>在文章展示时替换文字链</Description>
  <Author>&lt;a href="http://www.ebsite.net"&gt;小菜菜&lt;/a&gt;</Author>
  <AdminPage />
  <Enabled>true</Enabled>
  <Settings>
    <Index>0</Index>
    <Name>ReplaceOnShowInfo</Name>
    <Help>可以在内容发布时给一些关键字加上连接</Help>
    <Delimiter>44</Delimiter>
    <Parameters>
      <Name>ID</Name>
      <Label>唯一ID</Label>
      <MaxLength>20</MaxLength>
      <Required>true</Required>
      <KeyField>true</KeyField>
      <ParamType>String</ParamType>
      <SelectedValue />
    </Parameters>
    <Parameters>
      <Name>KeyWord</Name>
      <Label>添加关键字</Label>
      <MaxLength>100</MaxLength>
      <Required>true</Required>
      <KeyField>true</KeyField>
      <ParamType>String</ParamType>
      <SelectedValue />
    </Parameters>
    <Parameters>
      <Name>Url</Name>
      <Label>添加连接</Label>
      <MaxLength>150</MaxLength>
      <Required>true</Required>
      <KeyField>false</KeyField>
      <ParamType>String</ParamType>
      <SelectedValue />
    </Parameters>
    <Parameters>
      <Name>Description</Name>
      <Label>描述（可选）</Label>
      <MaxLength>100</MaxLength>
      <Required>false</Required>
      <KeyField>false</KeyField>
      <ParamType>String</ParamType>
      <SelectedValue />
    </Parameters>
    <KeyField>ID</KeyField>
    <IsScalar>false</IsScalar>
    <Hidden>false</Hidden>
    <ShowAdd>true</ShowAdd>
    <ShowEdit>true</ShowEdit>
    <ShowDelete>true</ShowDelete>
  </Settings>
  <ShowSettings>true</ShowSettings>
</ManagedExtension>